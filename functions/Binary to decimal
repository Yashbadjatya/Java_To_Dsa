    public static void bintodec(int binnum){
        int mynum = binnum;
        int pow = 0;
        int decnum = 0;

        while(binnum>0){
            int Lastdigit = binnum%10;
            decnum = decnum + (Lastdigit*(int)Math.pow(2, pow));

            pow++;
            binnum=binnum/10;
        }
        System.out.println("coverstion of is " + binnum + " = "  + decnum);
    }
